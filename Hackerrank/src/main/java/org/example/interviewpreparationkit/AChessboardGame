package org.example.interviewpreparationkit;

public class AChessboardGame {
    public static void main(String[] args) {
        System.out.println(chessboardGame(5, 2));
    }

    public static String chessboardGame(int x, int y) {
        if (((x - 1) / 2) % 2 == 0 && ((y - 1) / 2) % 2 == 0) {
            return "Second";
        } else {
            return "First";
        }
    }
}
